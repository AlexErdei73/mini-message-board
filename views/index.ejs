<%- include("_header") %>
<div class="container">
  <h1><%= title %></h1>
  <p>Welcome to <%= title %></p>
  <div class="row row-cols-1 row-cols-lg-2 row-cols-xl-3 gy-3 mb-3">
    <% messages.forEach(function(msg) { %> <%- include("_messagewidget", { msg:
    msg }) %> <% }) %>
  </div>
  <a href="/new" class="btn btn-primary">New message</a>
</div>

<%- include("_footer") %>
